{
  "name": "react-native-calendar-kit",
  "version": "0.1.0",
  "description": "A simple JS only calendar UI for react native",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/arbta/react-native-calendar-kit",
  "author": "Frank Owusu-Agyemang <codewithfrank@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "test": "TZ=UTC jest"
  },
  "dependencies": {
    "@shopify/flash-list": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@tsconfig/recommended": "^1.0.6",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "babel-jest": "^29.7.0",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "react": "^18.3.1",
    "react-native": "^0.74.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@shopify/flash-list": ">=1.6.4",
    "react": "*",
    "react-native": ">=0.73.8"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "dts": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react",
      "react-native",
      "react-native-web"
    ],
    "minify": true
  },
  "files": [
    "src/",
    "dist"
  ]
}
